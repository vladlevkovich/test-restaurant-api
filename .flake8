[flake8]
max-line-length = 88
extend-ignore = E203, W503
exclude = 
    .git,
    __pycache__,
    .venv,
    venv,
    env,
    .env,
    migrations,
    media,
    staticfiles,
    *.pyc,
    *.pyo,
    *.pyd,
    .pytest_cache,
    .mypy_cache
per-file-ignores =
    __init__.py:F401
    migrations/*:E501 